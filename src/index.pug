extends views/layout

block headcontent
	link(rel='stylesheet', href='https://cdn.jsdelivr.net/npm/water.css@2/out/water.css')

block content
	#tyn_root

		.full_page_container
			#tyn_header_root
				include:remarkable /res/text/main.md

		#tyn_gallery_root
			h1 🎨Gallery
			h2 Screenshots
			#tyn_gallery_screenshots(class='tyn_gallery_section')
				.content_row.move_left
			code 💡 More in my 
				a(href="https://instagram.com/tyndustre/") Instagram
				|  or 
				a(href="https://twitter.com/search?q=(%23portfolio_clips)%20(from%3Atynrare)&f=video") Twitter
				|  or 
				a(href="https://youtube.com/playlist?list=PL9r7mG-N0wRSnWNLvsWOIvzYBeGFHmqFO") Youtube

		#tyn_aboutme_root
			include:remarkable /res/text/aboutme.md
		#tyn_services_root
			include:remarkable /res/text/offers.md
		#tyn_links_root
			include:remarkable /res/text/links.md

		a(class='footer') 🍍🍍🍍
